[user]
    name = Michael Reed
    email = mhr2126@gmail.com
[branch]
    autosetuprebase = always
[push]
    default = simple
[alias]
        amend=commit --amend
        br = branch
        cam = commit -a -m
        co = checkout
        cob = checkout -b
        com = commit -m
        cp = cherry-pick
        hist = log --graph --pretty=format:'%C(yellow)%h%Creset -%C(bold red)%d%Creset %s %C(bold green)(%cr) %C(magenta bold)<%an>%Creset' --abbrev-commit --date=relative
        lo = log
        pl = pull --rebase
        ps = push
        st = status
        sh = show --color
        di = diff --color
        unstage = checkout HEAD
        wc = whatchanged -p
